all: directories library $(PROGRAM) $(PROGRAM2) $(PROGRAM3)

depend:
	sfmakedepend $(F1SRCS) $(F2SRCS) $(FORSRCS) $(F90SRCS) $(F90SRCS1) $(F03SRCS)

directories: $(BIN) $(LIB)

$(BIN): 
	mkdir -p $(BIN)

$(LIB):
	mkdir -p $(LIB)

library: $(LIBOBJS)
	$(AR) cr $(LIB)/$(LIBPREFIX)$(LIBRARY).$(LIBEXT) $(LIBOBJS)

$(PROGRAM): $(PODOBJS) 
	 $(FC) -o $(BIN)/$@ $(PODOBJS) $(FLFLAGS)

$(PROGRAM2): $(CRS2TRSOBJS) 
	$(FC) -o $(BIN)/$@ $(CRS2TRSOBJS) $(FLFLAGS)

$(PROGRAM3): $(BRDC2ECEFOBJS) 
	$(FC) -o $(BIN)/$@ $(BRDC2ECEFOBJS) $(FLFLAGS)

clean:
	rm -f *.o *.mod $(LIB)/lib$(LIBRARY).$(LIBEXT) $(BIN)/$(PROGRAM) $(BIN)/$(PROGRAM2) $(BIN)/$(PROGRAM3)

