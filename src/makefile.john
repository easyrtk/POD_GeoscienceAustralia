%.$(OBJ): %.F90
	$(FC) $(FCFLAGS) $(F90CFLAGS) -c $< -o $@
	-test -f $(*F).mod && touch $(*F).mod

%.mod: %.F90
	$(FC) $(FCFLAGS) $(F90CFLAGS) -c $<

%.$(OBJ): %.f90
	$(FC) $(FCFLAGS) $(F90CFLAGS) -c $< -o $@
	-test -f $(*F).mod && touch $(*F).mod

%.mod: %.f90
	$(FC) $(FCFLAGS) $(F90CFLAGS) -c $<

%.$(OBJ): %.f03
	$(FC) $(FCFLAGS) $(F03CFLAGS) -c $< -o $@
	-test -f $(*F).mod && touch $(*F).mod

%.mod: %.f03
	$(FC) $(FCFLAGS) $(F03CFLAGS) -c $<

%.$(OBJ): %.for
	$(FC) $(FCFLAGS) $(FORCFLAGS) -c $< -o $@
	-test -f $(*F).mod && touch $(*F).mod

%.mod: %.for
	$(FC) $(FCFLAGS) $(FORCFLAGS) -c $<

%.$(OBJ): %.f
	$(FC) $(FCFLAGS) $(FLEGACYCFLAGS) -c $< -o $@
	-test -f $(*F).mod && touch $(*F).mod

%.$(OBJ): %.F
	$(FC) $(FCFLAGS) $(FLEGACYCFLAGS) -c $< -o $@
	-test -f $(*F).mod && touch $(*F).mod

